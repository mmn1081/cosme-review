<div>
  <h1>新規投稿</h1>
  <%= form_with model: @post_cosme, url: public_post_cosmes_path do |f| %>
    <div>
      <%= f.label :"画像" %>
      <%= f.file_field :image, accept: "image/*" %>
    </div>
    <div>
      <%= f.label :"商品名" %>
      <%= f.text_field :cosme_name %>
    </div>
    <div>
      <%= f.label :"内容" %>
      <%= f.text_area :introduction %>
    </div>
    <%= f.submit '投稿' %>
  <% end %>
</div>
